{
  "bluprint": "^0.4.3",
  "name": "GitHub action scraper",
  "category": "",
  "actions": [{
    "action": "prompt",
    "questions": [{
      "type": "text",
      "name": "projectName",
      "message": "What should we call this project?"
    }]
  }, {
    "action": "remove",
    "paths": [
      "README.md"
    ]
  }, {
    "action": "move",
    "paths": [
      ["github/", ".github/"],
      ["README_template.md", "README.md"]
    ]
  }, {
    "action": "render",
    "engine": "mustache",
    "files": ["README.md"]
  }, {
    "action": "render",
    "engine": "mustache",
    "files": ["index.js"]
  },{
    "action": "regexreplace",
    "files": ["package.json"],
    "replace": [
      ["projectName", "action_{{#slugify}}{{ projectName }}{{/slugify}}"]
    ]
  }, {
    "action": "execute",
    "cmds": [
      ["yarn"],
      ["git", ["init"]]
    ],
    "silent": true
  }, {
    "action": "log",
    "msg": "{green Ready to build you scraper!} Check out the README for development instructions."
  }, {
    "action": "log",
    "msg": "Prefix the name of your git repo with {green action_}"
  }]
}
